{
  "expo": {
    "name": "Veda - Ayurvedic Diet Assistant",
    "slug": "Veda",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "veda",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#FEFAF5"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ayurveda.veda"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#FEFAF5",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.ayurveda.veda",
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#FEFAF5"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow Veda to access your photos to analyze food images.",
          "cameraPermission": "Allow Veda to access your camera to capture food photos."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "8ab0a9cb-e3ec-4531-87dd-28606e452fa7"
      }
    }
  }
}
